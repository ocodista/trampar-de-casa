{
  "name": "roles-renderer",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild ./src/index.ts --bundle --platform=node --target=node20.5.1 --outfile=./dist/index.js --define:import.meta.url=import_meta_url --external:isomorphic-fetch",
    "inspect": "node --inspect --no-experimental-fetch -r esbuild-register -r isomorphic-fetch",
    "start": "node --no-experimental-fetch -r esbuild-register -r isomorphic-fetch src/index.ts",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "test:silent": "vitest run --silent"
  },
