    "react": "^18.2.0",
    "react-email": "^1.9.4",
    "redis": "^4.6.7",
    "shared": "*",
    "vitest": "^0.34.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/react": "^18.2.15",
    "cross-fetch": "^4.0.0",
    "typescript": "^5.1.6",
    "es": "^0.8.0",
    "esbuild": "^0.19.5",
